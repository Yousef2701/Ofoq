</head>
<body>
    <div class="scroller"></div>
    <div class="d-flex justify-content-center align-items-center flex-wrap overflow-hidden">
        <section>
            <img src="~/Uploads/main.png" style="width:100%;height:100%;z-index: 0;position: absolute;top: 0;right: 0;" />
        </section>
        <div class="teachers-container mt-4">
            <h2 class="text-capitalize text-center w-100">مدرسين The Top</h2>
            <button class="move right" onclick="scrollContent('right')">
                <svg xmlns="http://www.w3.org/2000/svg"
                     width="16"
                     height="16"
                     fill="currentColor"
                     class="bi bi-arrow-bar-right"
                     viewBox="0 0 16 16">
                    <path fill-rule="evenodd"
                          d="M6 8a.5.5 0 0 0 .5.5h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 0 0-.708.708L12.293 7.5H6.5A.5.5 0 0 0 6 8m-2.5 7a.5.5 0 0 1-.5-.5v-13a.5.5 0 0 1 1 0v13a.5.5 0 0 1-.5.5" />
                </svg>
            </button>
            <div class="scroll-x row align-items-center" id="content">
                @foreach (Teacher item in @ViewBag.Teachers)
                {
                    <form asp-controller="Home" asp-action="TeacherPage" method="get">
                        <input hidden asp-for="Id" value="@item.Id" />
                        <button type="submit" class="card-2">
                            <img src="~/UsersImages/@item.Image_Url" class="card-img-top" />
                            <div class="card-body">
                                <ul class="list-group list-group-flush">
                                    <li class="list-group-item">
                                        <span class="strong"> أ/ @item.Name  -  مدرس @item.Subject </span>
                                    </li>
                                    @if (item.First == true & item.Second == true & item.Third == true)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الاول و الثاني و
                                            الثالث الثانوي
                                        </li>
                                    }
                                    else  @if (item.First == true & item.Second == true & item.Third == false)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الاول و الثاني الثانوي
                                        </li>
                                    }
                                    else  @if (item.First == true & item.Second == false & item.Third == true)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الاول و الثالث الثانوي
                                        </li>
                                    }
                                    else  @if (item.First == false & item.Second == true & item.Third == true)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الثاني و الثالث الثانوي
                                        </li>
                                    }
                                    else  @if (item.First == true & item.Second == false & item.Third == false)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الاول الثانوي
                                        </li>
                                    }
                                    else  @if (item.First == false & item.Second == true & item.Third == false)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الثاني الثانوي
                                        </li>
                                    }
                                    else  @if (item.First == false & item.Second == false & item.Third == true)
                                    {
                                        <li class="list-group-item">
                                            <span class="strong">الصف </span> الثالث الثانوي
                                        </li>
                                    }

                                </ul>
                            </div>
                        </button>
                    </form>
                }

            </div>
            <button class="move left" onclick="scrollContent('left')">
